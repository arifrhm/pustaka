{
  "name": "kalender-jawa",
  "version": "1.0.0-alpha",
  "description": "Pustaka penanggalan dan perhitungan di dalam kalendar jawa",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "npx babel src --out-dir lib --ignore 'src/**/*.spec.js','src/**/*.test.js'",
    "build-browser": "rollup src/index.js --file lib/bundle.js --format esm --name 'Sengkala'",
    "doc": "npx jsdoc -c jsdoc.conf.json -r src -d docs",
    "test": "npm run build && npx jest --no-cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/junwatu/kalender-jawa.git"
  },
  "keywords": [
    "kalender",
    "jawa",
    "lambang",
    "windu",
    "pawukon",
    "sengkala",
    "kalender jawa",
    "pasaran",
    "candra sengkala"
  ],
  "author": "Equan Pr.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/junwatu/kalender-jawa/issues"
  },
  "homepage": "https://github.com/junwatu/kalender-jawa#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-flow": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "flow-bin": "^0.102.0",
    "jest": "^24.7.1",
    "jsdoc": "^3.6.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3"
  },
  "dependencies": {
    "core-js": "^3.1.4",
    "regenerator-runtime": "^0.13.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "mjs"
    ],
    "transform": {
      "^.+\\.m?js?$": "babel-jest"
    }
  }
}

{
  "name": "@kalenderjawa/pustaka",
  "version": "1.0.0-beta.10",
  "description": "Pustaka JavaScript untuk perhitungan, pengkonversian dan penanggalan Kalender Jawa Abadi",
  "main": "lib/kalenderjawa.min.js",
  "type": "module",
  "module": "lib/kalenderjawa.min.mjs",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npx webpack && npm run build-native-module",
    "build-babel": "npx babel src --out-dir lib --ignore \"src/**/*.spec.js\",\"src/**/*.test.js\"",
    "build-native-module": "rollup --config",
    "doc": "npx jsdoc -c jsdoc.conf.json -r src -d docs",
    "test": "npx jest --no-cache --coverage --testPathIgnorePatterns=\"_data.*\"",
    "test-watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kalenderjawa/pustaka.git"
  },
  "keywords": [
    "kalender",
    "jawa",
    "lambang",
    "windu",
    "pawukon",
    "sengkala",
    "kalender jawa",
    "pasaran",
    "candra sengkala",
    "penanggalan",
    "dinten",
    "dino",
    "tahun jawa",
    "penanggalan jawa",
    "kalender jawa abadi",
    "javascript",
    "api",
    "library"
  ],
  "author": "Equan Pr.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kalenderjawa/pustaka/issues"
  },
  "homepage": "https://github.com/kalenderjawa/pustaka#readme",
  "devDependencies": {
    "@babel/cli": "^7.11.5",
    "@babel/core": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-flow": "^7.10.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.5.1",
    "babel-loader": "^8.1.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "flow-bin": "^0.132.0",
    "jest": "^25.5.4",
    "jsdoc": "^3.6.5",
    "rollup-plugin-flow": "^1.1.1",
    "rollup-plugin-terser": "^7.0.1",
    "standard": "^14.3.4",
    "webpack": "^5.0.0-beta.29",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "regenerator-runtime": "^0.13.7"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/lib/"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "mjs"
    ],
    "transform": {
      "^.+\\.m?js?$": "babel-jest"
    }
  },
  "standard": {
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ],
    "ignore": [
      "lib"
    ],
    "env": [
      "jest"
    ]
  }
}
